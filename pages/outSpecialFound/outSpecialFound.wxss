.outSpecialFound {
    padding: 30rpx
}

.item {
    background-color: #fff;
    display: -webkit-flex;
    display: flex;
    -webkit-flex-direction: column;
    flex-direction: column
}

.item-con,.item-footer,.item-header {
    -webkit-align-items: center;
    align-items: center;
    display: -webkit-flex;
    display: flex;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    padding: 30rpx
}

.item-con {
    color: #a9a9a9;
    font-size: 26rpx
}

.item-header {
    border-bottom: 1px solid #f5f6f7
}

.item-footer {
    color: #369afb;
    -webkit-justify-content: flex-end;
    justify-content: flex-end
}

.item-header-img {
    height: 80rpx;
    width: 80rpx
}

.item-header-status {
    -webkit-align-items: center;
    align-items: center;
    border-radius: 30rpx;
    color: #fff;
    display: -webkit-flex;
    display: flex;
    font-size: 26rpx;
    height: 40rpx;
    -webkit-justify-content: center;
    justify-content: center;
    width: 100rpx
}

.statusTrue {
    background-color: #56d79e
}

.statusFalse {
    background-color: #ff8174
}

.item-con-img {
    height: 30rpx;
    width: 30rpx
}

.outSpecialFound-none {
    color: #a9a9a9;
    margin-top: 100rpx;
    text-align: center
}
